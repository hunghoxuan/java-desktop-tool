<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
<Module ModuleName = "clearing_clients"> 
	<TableExtractDefinition TableName="cis_client*" columnName="client_number"> 
		<WhereClause>client_number in ('90000002')
		</WhereClause>
	</TableExtractDefinition>	
	<TableExtractDefinition TableName="cas_*"  columnName="acct_number"> 
		<WhereClause> (acct_number like '90000001%' or acct_number like '90000002%')
		</WhereClause>
	</TableExtractDefinition>

	<TableExtractDefinition TableName="cas*"  columnName="account_number"> 
		<WhereClause> (account_number like '90000001%' or account_number like '90000002%')
		</WhereClause>
	</TableExtractDefinition>
	
	<TableExtractDefinition TableName="SVC_*" columnName="client_number"> 
		<WhereClause>client_number in ('90000002')
		</WhereClause>
	</TableExtractDefinition>	
	
</Module>

<Module ModuleName = "SuspenseClient"> 
	<TableExtractDefinition TableName="cis_client*" columnName="client_number"> 
		<WhereClause>client_number = institution_number
		</WhereClause>
	</TableExtractDefinition>	
	<TableExtractDefinition TableName="cas_*"  columnName="acct_number"> 
		<WhereClause> (acct_number like institution_number||'%' or acct_number like institution_number||'%')
		</WhereClause>
	</TableExtractDefinition>

	<TableExtractDefinition TableName="cas*"  columnName="account_number"> 
		<WhereClause> (account_number like institution_number||'%' or account_number like institution_number||'%')
		</WhereClause>
	</TableExtractDefinition>
</Module>
<!--
-->

</root>