<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
	<Module ModuleName = "clients"> 
		<TableExtractDefinition TableName="cis_client*" columnName="client_number"> 
			<WhereClause>client_number in ('00000036')
			</WhereClause>
		</TableExtractDefinition>	
		<TableExtractDefinition TableName="cas_*"  columnName="acct_number"> 
			<WhereClause> (acct_number like '00000036%' or acct_number like '00000036%')
			</WhereClause>
		</TableExtractDefinition>

		<TableExtractDefinition TableName="SVC_*" columnName="client_number"> 
			<WhereClause>client_number in ('00000036')
			</WhereClause>
		</TableExtractDefinition>	
	</Module>
</root>