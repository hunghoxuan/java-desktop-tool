<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>


<Module ModuleName = "clearAndSuspSetup"> 

<TableExtractDefinition TableName="CHT_SERVICE_CONTRACT_ID"> 
<WhereClause> index_Field in ('202','990')</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_SERVICE_CONTRACT"> 
<WhereClause> service_contract_id in ('202','990')</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_ASSIGNED_SERVICES"> 
<WhereClause> service_contract_id in ('202')  and SERVICE_CATEGORY='003'</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_SERVICE_DEFINITION"> 
<WhereClause> (INSTITUTION_NUMBER,SERVICE_ID) in (select INSTITUTION_NUMBER,SERVICE_ID from CBR_ASSIGNED_SERVICES where service_contract_id in ('202')  and SERVICE_CATEGORY='003')</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CHT_SERVICES"> 
<WhereClause> (INSTITUTION_NUMBER,index_field) in (select INSTITUTION_NUMBER,SERVICE_ID from CBR_ASSIGNED_SERVICES where service_contract_id in ('202')  and SERVICE_CATEGORY='003')</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_POSTING_INSTRUCTIONS"> 
<WhereClause> service_contract_id in ('202','990')  and ACCT_CURRENCY='978'</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_ACCOUNT_TYPE"> 
<WhereClause>  ACCOUNT_TYPE_ID in ('091','092','075','076','111','120')</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_CONTRACT_ACCT_TYPES"> 
<WhereClause> service_contract_id in ('202','990') and acct_currency='978' </WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_SETTLEMENT_SCHEMES"> 
<WhereClause> service_contract_id in ('202','990') and acct_currency='978'</WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CBR_TRANSACTION_CHARGES"> 
<WhereClause> service_contract_id in ('202','990') </WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CHT_ACCOUNT_CONDITION_SET"> 
<WhereClause> INDEX_FIELD in ('908','001') </WhereClause>
</TableExtractDefinition>

<TableExtractDefinition TableName="CHT_ACCOUNT_TYPE_ID"> 
<WhereClause> (INDEX_FIELD,institution_number) in (select account_Type_id, institution_number from CBR_CONTRACT_ACCT_TYPES where service_contract_id in ('202','990') and acct_currency='978') </WhereClause>
</TableExtractDefinition>


<TableExtractDefinition TableName="CBR_CHART_OF_ACCOUNTS"> 
<WhereClause>  (service_contract_id in ('202','990') or ACCOUNT_TYPE_ID in ('091','092','075','076','111','120')) and gl_acct_currency='978'
</WhereClause>
</TableExtractDefinition>

</Module>


</root>